   "source": [
    "**Write a program which can compute the factorial of a number.**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "b44dd4af",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter your number: 5\n",
      "\n",
      "Factorial: 120\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter your number: \"))\n",
    "f = 1\n",
    "\n",
    "if n < 0:\n",
    "    print (\"\\nInvalid Factorial\")\n",
    "elif n == 0:\n",
    "    print (\"\\nFactorial: 1\")\n",
    "else:\n",
    "    for i in range(1, n + 1):\n",
    "        f = f * i\n",
    "    print (\"\\nFactorial:\", f)"
   ]
  }
 ],
