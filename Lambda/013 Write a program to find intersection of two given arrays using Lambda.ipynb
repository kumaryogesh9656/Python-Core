     "output_type": "stream",
     "text": [
      "Original arrays:\n",
      "[1, 2, 3, 5, 7, 8, 9, 10]\n",
      "[1, 2, 4, 8, 9]\n",
      "\n",
      "Intersection of the said arrays: [1, 2, 8, 9]\n"
     ]
    }
   ],
   "source": [
    "a1 = [1, 2, 3, 5, 7, 8, 9, 10]\n",
    "a2 = [1, 2, 4, 8, 9]\n",
