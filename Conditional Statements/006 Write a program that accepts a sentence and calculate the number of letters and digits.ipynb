   "source": [
    "**Write a program that accepts a sentence and calculate the number of letters and digits.**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "dc093e5b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter your sentence: hello world! 123\n",
      "Letters: 10\n",
      "Digits : 3\n"
     ]
    }
   ],
   "source": [
    "n = input(\"Enter your sentence: \")\n",
    "l, d = 0, 0\n",
    "\n",
    "for i in n:\n",
    "    if i.isalpha():\n",
    "        l += 1\n",
    "    elif i.isdigit():\n",
    "        d += 1\n",
    "\n",
    "print (\"Letters:\", l)\n",
    "print (\"Digits :\", d)"
   ]
  }
 ],
