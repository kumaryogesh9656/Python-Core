     "output_type": "stream",
     "text": [
      "20\n"
     ]
    }
   ],
   "source": [
    "def sum(a, b):\n",
    "    c = a + b\n",
    "    \n",
    "    if c in range(15, 20):\n",
    "        print (20)    \n",
    "    else:\n",
    "        print (c)\n",
    "    \n",
    "sum(10, 9)    "
   ]
