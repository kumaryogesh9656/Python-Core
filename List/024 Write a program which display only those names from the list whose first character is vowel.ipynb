   "source": [
    "**Write a program which display only those names from the list whose first character is vowel.**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "bcb4f7b5",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Amit\n",
      "Uma\n",
      "Emil\n"
     ]
    }
   ],
   "source": [
    "l = [\"Amit\", \"Suman\", \"Sumit\", \"Uma\", \"Emil\"]\n",
    "\n",
    "for i in l:\n",
    "    if i[0].lower() in \"aeiou\":\n",
    "        print (i)"
   ]
  }
