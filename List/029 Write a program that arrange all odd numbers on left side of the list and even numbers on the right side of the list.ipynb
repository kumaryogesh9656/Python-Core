   "source": [
    "**Write a program that arrange all odd numbers on left side of the list and even numbers on the right side of the list.**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "cc42f4f5",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Original List: [23, 44, 65, 78, 34, 21, 9]\n",
      "\n",
      "[9, 21, 65, 23, 44, 78, 34]\n"
     ]
    }
   ],
   "source": [
    "l = [23, 44, 65, 78, 34, 21, 9]\n",
    "print (f\"Original List: {l}\\n\")\n",
    "\n",
    "for i in range(len(l)):\n",
    "    if l[i] % 2 != 0:\n",
    "        l.insert(0, l.pop(i))\n",
    "    \n",
    "print (l)"
   ]
  }
 ],
